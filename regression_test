#/bin/bash
for cir in x2 classifier fft mult16 adder32 mac mult8u buttfly abs_diff
do
	echo -n ${cir} " "
	cd ${cir}
	iverilog ${cir}_test.v ${cir}.v
	./a.out > acc_res
        iverilog ${cir}_test.v ${cir}_approx.v
        ./a.out > approx_res
	python3 ${cir}_qor.py acc_res approx_res
	rm a.out
	rm *res*
	cd ..
done
